{
  "name": "membership-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mui/material": "^5.1.0",
    "autoprefixer": "^10.4.0",
    "next": "11.1.2",
    "next-auth": "^3.29.0",
    "postcss": "^8.4.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tailwindcss": "^2.2.19"
  },
  "devDependencies": {
    "@types/react": "17.0.27",
    "cz-conventional-changelog": "^3.0.1",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "husky": "^7.0.0",
    "lint-staged": "^11.2.2",
    "typescript": "4.4.3"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
